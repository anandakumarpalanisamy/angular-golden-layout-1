{
    "compilerOptions": {
      "baseUrl": ".",
      "declaration": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "inlineSources": true,
      "lib": [
        "es5",
        "dom"
      ],
      "module": "es5",
      "moduleResolution": "node",
      "outDir": "../dist",
      "rootDir": ".",
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": false,
      "target": "es5",
      "typeRoots": [
        "../node_modules/@types"
      ],
      "types": [
        "node",
        "golden-layout"
      ]
    },
    "files": [
      "index.ts"
    ],
    "angularCompilerOptions": {
      "annotateForClosureCompiler": true,
      "flatModuleId": "angular-golden-layout",
      "flatModuleOutFile": "angular-golden-layout.js",
      "skipTemplateCodegen": true,
      "strictMetadataEmit": true
    }
  }